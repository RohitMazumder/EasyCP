%YAML 1.2
---
name: EasyCP
scope: text.plain

contexts:
  main:
    - match: "\\*+\\s*Executing Test-Case\\s*\".+\"\\s*\\*+"
      scope: string
    - match: "(Input|Expected Output|Your Output):\\n+"
      scope: storage.type
    - match: FAILED
      scope: keyword
    - match: Passed Successfuly
      scope: markup.inserted